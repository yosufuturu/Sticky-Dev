{
  "name": "sticky-dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://yosufuturu.github.io/sticky-dev/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
